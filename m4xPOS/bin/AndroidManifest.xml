<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mqm0.m4xPOS"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <library 
        android:name="@string/lib"/>

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/icon18"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.pofs.Uploading"
            android:icon="@drawable/ic_action_import_export"
            android:label="@string/title_activity_asynctask"
            android:theme="@android:style/Theme.Holo.Dialog" 
            >
        </activity>
        <activity
            android:name="com.pofs.ListCommandesActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pofs.ProductsActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity 
            android:name="com.pofs.Initializing"
            android:icon="@drawable/ic_action_rotate_left"
            android:label="@string/title_activity_to_clock_in">
        </activity>
    </application>

</manifest>